    <PackageReference Include="EPPlus" Version="5.8.14" />
    <PackageReference Include="CsvHelper" Version="27.2.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DocExtractor.Core\DocExtractor.Core.csproj" />
    <ProjectReference Include="..\DocExtractor.ML\DocExtractor.ML.csproj" />
  </ItemGroup>
</Project>
